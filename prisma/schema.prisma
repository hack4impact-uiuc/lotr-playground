generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Character {
  id        Int      @id @default(autoincrement())
  name      String   @default("")
  gender    String   @default("")
  race      String   @default("")
  realm     String   @default("")
  wikiUrl   String   @default("")
  imageUrl  String   @default("")
}
